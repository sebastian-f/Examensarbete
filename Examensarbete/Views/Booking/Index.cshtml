@model Examensarbete.Models.MakeBooking
<link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
<style>
    td{
          padding:5px;
    }
</style>
@{
    ViewBag.Title = "Index";
}

<h2>Välj datum</h2>
<br />

@using (Html.BeginForm())
{
    <div class="form-group">
        @Html.Label("Incheckning")
        @Html.TextBoxFor(m => m.CheckInDate, "{0:yyyy-MM-dd}", new { @class = "datepicker form-control" })
    </div>
    <div class="form-group">
        @Html.Label("Utcheckning")
        @Html.TextBoxFor(m => m.CheckOutDate, "{0:yyyy-MM-dd}", new { @class = "datepicker form-control" })
    </div>
       <input type="submit" class="btn btn-info" value="Nästa" />
    
}

@section scripts
{
    <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>

<script>

    $(function () {
        $(".datepicker").datepicker({dateFormat: 'yy-mm-dd' });
    });
</script>

    }